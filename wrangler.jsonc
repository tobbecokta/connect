{
  "name": "connect",
  "compatibility_date": "2025-04-10",
  "assets": {
    "directory": "./dist"
  },
  "route": "",
  "build": {
    "command": "npm run build",
    "directory": "dist"
  },
  "site": {
    "bucket": "dist"
  },
  "env": {
    "SUPABASE_URL": "SUPABASE_URL",
    "SUPABASE_SERVICE_ROLE_KEY": "SUPABASE_SERVICE_ROLE_KEY"
  },
  "routes": [
    {
      "pattern": "*",
      "zone_name": "connect"
    }
  ]
} 